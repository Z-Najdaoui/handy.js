{
  "name": "@handy.js/handy",
  "version": "1.0.1",
  "type": "module",
  "description": "A set of useful modules that makes the javascript development context easier",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/handy-js/handy.git"
  },
  "main": "dist/handy.min.cjs",
  "module": "dist/handy.min.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "rollup -c rollup.config.js --config-dev -w",
    "build": "rollup -c rollup.config.js --config-prod"
