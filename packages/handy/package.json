    "handy.js",
    "handy-js"
  ],
  "author": "handy.js",
  "links": {
    "npm": "https://www.npmjs.com/package/@handy.js/handy",
    "github": "https://github.com/handy-js/handy/tree/main/packages/handy"
  },
  "license": "MIT",
  "devDependencies": {
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
